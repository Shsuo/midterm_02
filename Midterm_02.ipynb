    "def my_sd(the_list):\n",
    "         \n",
    "    sdd = 0\n",
    "    for i in the_list:\n",
    "        sdd += (i - sum(the_list)/len(the_list))**2\n",
    "    \n",
    "    return(sdd/len(the_list))**(0.5)\n",
    "    \n",
    "my_list = [765,544,689,609,36,381,689,151]\n",
    "my_sd(my_list)\n",
